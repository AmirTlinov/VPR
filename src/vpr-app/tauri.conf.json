{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "VPR",
  "version": "0.1.0",
  "identifier": "com.vpr.client",
  "build": {
    "frontendDist": "frontend",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "VPR",
        "width": 380,
        "height": 520,
        "minWidth": 320,
        "minHeight": 400,
        "resizable": true,
        "center": true,
        "decorations": true,
        "transparent": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
